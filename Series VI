import java.util.Scanner;
public class Series6
{
    public static void main(String[] args)
    {
        Scanner sc=new Scanner(System.in);
        int a=sc.nextInt();
        int b,b1;
        for(int i=1;i<=a;i++)
        {
            if(i%2==0)
            {
                b=(i*i)-2;
            }
            else
            {
                b=(i*i)-1;
            }
            System.out.println(b);
            }
            }
        }
