import java.util.*;
public class Reverse
{
    public static void main(String[] args)
    {
       Scanner sc=new Scanner(System.in);
     int  n=sc.nextInt();
      int a=n,reverse=0;
      while(a!=0)
      {
          int remainder=a%10;
          reverse=reverse*10+remainder;
          a=a/10;
      }
      System.out.println(" " +reverse);
    }
}
