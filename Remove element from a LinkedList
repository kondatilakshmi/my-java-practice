import java.util.*;
public class collection
{
    public static void main(String args[])
    {
        LinkedList a=new LinkedList();
        a.add("Java");
         a.add("Python");
          a.add("Javascript");
          a.add("Kotlin");
          System.out.println("LinkedList:"+a);
         
           System.out.println("Removed Element:"+a.get(1));
            a.remove(1);
          System.out.println("New LinkedList:"+a);
    }
}
