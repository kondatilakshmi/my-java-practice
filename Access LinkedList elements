import java.util.*;
public class collection
{
    public static void main(String[]args)
    {
    LinkedList al=new LinkedList();
    al.add("Python");
    al.add("Java");
    al.add("Javascript");
    System.out.println("LinkedList:"+al);
    System.out.println("Element at index 1:"+al.get(1));
    }

}
