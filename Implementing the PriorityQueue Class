//Code here
import java.util.*;
public class Main{
    public static void main(String[] args)
    {
        Queue <Integer> a= new LinkedList<>();
        a.add(1);
        a.add(5);
        a.add(2);
        System.out.println("Queue:"+a);
        a.remove(5); a.remove(2);
        System.out.println("Accessed Element:"+a);
        System.out.println("Removed Element:"+a);
        a.add(5);a.add(2);a.remove(1);
        System.out.println("Updated Queue:"+a);
    }
}
