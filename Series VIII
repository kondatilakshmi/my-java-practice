import java.util.*;
public class Series8
{
    public static void main(String[] args)
    {
        Scanner sc=new Scanner(System.in);
        int a=sc.nextInt();
        int b=4;
        int c;
        for(int i=0;i<a;i++)
        {
            c=i*i;
            b=b+c;
            System.out.println(b);
        }
    }
}
