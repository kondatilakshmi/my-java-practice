import java.util.Scanner;
public class Series10
{
    public static void main(String[] args)
    {
        Scanner sc=new Scanner(System.in);
        int a=sc.nextInt();
        double b=95.0;
        System.out.print(b+" ");
        double c=20.5;
        double d;
        for(int i=1;i<a;i++)
        {
            d=(b+c);
            System.out.print(d+" ");
            b=d;
            c=c+2;
        }
    }
}
