import java.util.Scanner;
public class Series9
{
    public static void main(String[] args)
    {
        Scanner sc=new Scanner(System.in);
        int a=sc.nextInt();
        long b=2;
        System.out.print(b+" ");
        for(int i=1;i<a;i++)
        {
            b=(b*b)-1;
            System.out.print(b+" ");
        }
    }
}
