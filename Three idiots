import java.util.*;
public class Three
{
    public static void main(String args[])
    {
 Scanner sc=new Scanner(System.in);
 int x1,x2,y1,y2;
 x1=sc.nextInt();
 y1=sc.nextInt();
 x2=sc.nextInt();
 y2=sc.nextInt();
 float a=(x1+x2)/2;
 double b=(y1+y2)/2.0;
 System.out.printf("binoy's house is located at(%.1f,%.1f)",a,b);
    }
}
